<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_wedding_trio_list" model="ir.ui.view">
        <field name="name">wedding.trio.list</field>
        <field name="model">wedding.trio</field>
        <field name="arch" type="xml">
            <list string="Wedding Trios" default_order="project_id,name">
                <field name="date"/>
                <field name="project_id" optional="hide"/>
                <field name="name"/>
                <field name="time"/>
                <field name="port_ids" widget="many2many_tags"/>
            </list>
        </field>
    </record>

    <record id="action_wedding_trio" model="ir.actions.act_window">
        <field name="name">Wedding Trios</field>
        <field name="res_model">wedding.trio</field>
        <field name="view_mode">list</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Wedding Trio kayıtlarını burada yönetin.
            </p>
        </field>
    </record>

