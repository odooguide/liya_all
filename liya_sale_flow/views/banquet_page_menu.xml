<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="view_banquet_pages_list" model="ir.ui.view">
    <field name="name">banquet.pages.list</field>
    <field name="model">banquet.pages</field>
    <field name="arch" type="xml">
      <list string="Banquet Page Names" editable="bottom">
        <field name="sequence"/>
        <field name="name"/>
      </list>
    </field>
  </record>

  <record id="view_banquet_pages_form" model="ir.ui.view">
    <field name="name">banquet.pages.form</field>
    <field name="model">banquet.pages</field>
    <field name="arch" type="xml">
      <form string="Banquet Page">
        <sheet>
          <group>
            <field name="sequence"/>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_banquet_pages" model="ir.actions.act_window">
    <field name="name">Banquet Pages</field>
    <field name="res_model">banquet.pages</field>
    <field name="view_mode">list,form</field>
    <field name="view_id" ref="view_banquet_pages_list"/>
  </record>
   <menuitem
    id="menu_banquet_pages"
    name="Banquet Pages"
    parent="crm.crm_menu_config"
    sequence="20"
  />
  <menuitem
    id="menu_banquet_pages_menu"
    name="Banquet Pages"
    parent="menu_banquet_pages"
    action="action_banquet_pages"
    sequence="20"
  />


</odoo>
